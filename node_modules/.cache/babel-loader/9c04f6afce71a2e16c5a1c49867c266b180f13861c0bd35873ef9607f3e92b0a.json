{"ast":null,"code":"import axios from \"axios\";\nimport { authService } from \"@/services/auth.service\";\nexport default {\n  name: 'RegisterItem',\n  data() {\n    return {\n      name: '',\n      email: '',\n      password: ''\n    };\n  },\n  methods: {\n    async handleSubmit() {\n      try {\n        const {\n          res\n        } = await axios.post('register', {\n          'name': this.name,\n          'email': this.email,\n          'password': this.password\n        });\n        authService.setAuthToken(res.data.token);\n      } catch (e) {\n        return Promise.reject(e);\n      }\n      console.log(authService.getAuthToken());\n    }\n  }\n};","map":{"version":3,"names":["axios","authService","name","data","email","password","methods","handleSubmit","res","post","setAuthToken","token","e","Promise","reject","console","log","getAuthToken"],"sources":["/Users/vitalij/PhpstormProjects/studyFront/src/components/Register.vue"],"sourcesContent":["<template>\n    <form @submit.prevent=\"handleSubmit\">\n        <h3>Sign up</h3>\n\n        <div class=\"form-group\">\n            <label>Name</label>\n            <input type=\"text\" class=\"form-control\" v-model=\"name\" placeholder=\"Your name\">\n        </div>\n\n        <div class=\"form-group\">\n            <label>Email</label>\n            <input type=\"email\" class=\"form-control\" v-model=\"email\" placeholder=\"Your mail\">\n        </div>\n\n        <div class=\"form-group\">\n            <label>Password</label>\n            <input type=\"password\" class=\"form-control\" v-model=\"password\" placeholder=\"Your password\">\n        </div>\n\n        <button class=\"btn btn-primary btn-block\">Sign up</button>\n    </form>\n</template>\n\n<script>\n\n    import axios from \"axios\";\n    import {authService} from \"@/services/auth.service\";\n\n    export default {\n        name: 'RegisterItem',\n        data() {\n            return {\n                name: '',\n                email: '',\n      